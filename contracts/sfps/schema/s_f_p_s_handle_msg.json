{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SFPSHandleMsg",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "append_subsequent_hashes"
      ],
      "properties": {
        "append_subsequent_hashes": {
          "type": "object",
          "required": [
            "committed_hashes"
          ],
          "properties": {
            "committed_hashes": {
              "$ref": "#/definitions/CommittedHashes"
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "Commit": {
      "type": "array",
      "items": {
        "type": "integer",
        "format": "uint8",
        "minimum": 0.0
      }
    },
    "CommittedHashes": {
      "type": "object",
      "required": [
        "commit",
        "hashes"
      ],
      "properties": {
        "commit": {
          "$ref": "#/definitions/Commit"
        },
        "hashes": {
          "$ref": "#/definitions/Hashes"
        }
      }
    },
    "Hashes": {
      "type": "object",
      "required": [
        "first_hash",
        "following_hashes"
      ],
      "properties": {
        "first_hash": {
          "type": "array",
          "items": {
            "type": "integer",
            "format": "uint8",
            "minimum": 0.0
          }
        },
        "following_hashes": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/HeaderHashWithHeight"
          }
        }
      }
    },
    "HeaderHashWithHeight": {
      "type": "object",
      "required": [
        "hash",
        "height"
      ],
      "properties": {
        "hash": {
          "type": "array",
          "items": {
            "type": "integer",
            "format": "uint8",
            "minimum": 0.0
          }
        },
        "height": {
          "type": "integer",
          "format": "int64"
        }
      }
    }
  }
}
