      "$ref": "#/definitions/Config"
    }
  },
  "definitions": {
    "Config": {
      "type": "object",
      "required": [
        "bitcoin_spv",
        "finance_admin",
        "sfps"
      ],
      "properties": {
        "bitcoin_spv": {
          "$ref": "#/definitions/ContractReference"
        },
        "finance_admin": {
          "$ref": "#/definitions/ContractReference"
        },
        "sfps": {
          "$ref": "#/definitions/ContractReference"
        }
      }
    },
    "ContractReference": {
      "type": "object",
      "required": [
        "address",
        "hash"
      ],
      "properties": {
        "address": {
          "$ref": "#/definitions/HumanAddr"
        },
        "hash": {
          "type": "string"
        }
      }
    },
    "HumanAddr": {
      "type": "string"
    }
  }
}
